  "keywords": [
    "homebridge",
    "homebridge-plugin",
    "homebridge-panasonic-ac-platform",
    "homekit",
    "panasonic",
    "ac",
    "air conditioner",
    "comfort cloud"
  ],
  "dependencies": {
    "axios": "^1.7.2",
    "axios-cookiejar-support": "^5.0.2",
    "cheerio": "^1.0.0-rc.12",
    "jssha": "^3.3.1",
    "tough-cookie": "^4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.2.0",
    "eslint": "^9.9.0",
    "homebridge": "^2.0.0-beta.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1"
  }
